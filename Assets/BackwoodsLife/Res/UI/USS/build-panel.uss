.req-item-cont {
    flex-grow: 0;
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 1px;
    border-left-width: 0;
    border-top-left-radius: 0;
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
    border-bottom-left-radius: 0;
    width: 58px;
    height: 20px;
    align-items: center;
    justify-content: center;
    border-left-color: rgba(0, 0, 0, 0.13);
    border-right-color: rgba(0, 0, 0, 0.13);
    border-top-color: rgba(0, 0, 0, 0.13);
    border-bottom-color: rgba(0, 0, 0, 0.13);
    margin-bottom: 2px;
    white-space: nowrap;
    text-overflow: clip;
    -unity-text-align: middle-center;
    flex-direction: row;
}

.req-other-title-label {
    height: 20px;
    align-items: center;
    justify-content: center;
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    font-size: 10px;
    -unity-text-align: middle-center;
    white-space: nowrap;
    border-bottom-width: 1px;
    border-left-color: rgba(0, 0, 0, 0.13);
    border-right-color: rgba(0, 0, 0, 0.13);
    border-top-color: rgba(0, 0, 0, 0.13);
    border-bottom-color: rgba(0, 0, 0, 0.13);
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
}

.req-other-container {
    flex-grow: 0;
    width: 64px;
    height: 96px;
    margin-top: 2px;
    margin-right: 2px;
    margin-bottom: 2px;
    margin-left: 2px;
    border-top-left-radius: 6px;
    border-top-right-radius: 6px;
    border-bottom-right-radius: 6px;
    border-bottom-left-radius: 6px;
    border-left-color: rgba(0, 0, 0, 0.13);
    border-right-color: rgba(0, 0, 0, 0.13);
    border-top-color: rgba(0, 0, 0, 0.13);
    border-bottom-color: rgba(0, 0, 0, 0.13);
    border-top-width: 1px;
    border-right-width: 1px;
    border-bottom-width: 1px;
    border-left-width: 1px;
    align-items: center;
    justify-content: center;
}

.res-item-icon {
    flex-grow: 0;
    width: 22px;
    height: 22px;
    background-image: url("project://database/Assets/BackwoodsLife/Res/Images/Icons/Resource/blueberry-icon.png?fileID=2800000&guid=dab5438820d7763429ed7ca8451a27ca&type=3#blueberry-icon");
}

.res-item-count-label {
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
    font-size: 8px;
    -unity-font: url("project://database/Assets/BackwoodsLife/Res/Fonts/Dosis/Dosis-Bold.ttf?fileID=12800000&guid=22ec7a19c145b4a4788639f48c32710b&type=3#Dosis-Bold");
    white-space: nowrap;
    text-overflow: clip;
    -unity-text-align: middle-center;
}

.res-item-count-container {
    flex-grow: 0;
    width: 36px;
    height: 14px;
    -unity-text-align: middle-center;
    align-items: center;
    justify-content: center;
}

.res-item {
    flex-grow: 0;
    height: 36px;
    width: 36px;
    align-items: center;
    justify-content: flex-start;
    margin-left: 2px;
    margin-right: 2px;
}

.is-enough-icon-container {
    flex-grow: 1;
    width: 12px;
    height: 12px;
    position: absolute;
    top: -3px;
    right: -3px;
    align-items: center;
    justify-content: center;
}

.req-item {
    flex-grow: 0;
    height: 20px;
    left: auto;
    right: auto;
    width: 110px;
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    position: relative;
    flex-direction: row;
    font-size: 10px;
}

.crafting-btn {
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
    margin-top: 5px;
    margin-right: 5px;
    margin-bottom: 5px;
    margin-left: 5px;
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 2px;
    border-left-width: 0;
    border-top-left-radius: 6px;
    border-top-right-radius: 6px;
    border-bottom-right-radius: 6px;
    border-bottom-left-radius: 6px;
    flex-direction: row;
    font-size: 9px;
    background-color: rgb(111, 204, 140);
    height: 30px;
    border-bottom-color: rgb(66, 121, 83);
    border-left-color: rgb(66, 121, 83);
    border-right-color: rgb(66, 121, 83);
    border-top-color: rgb(66, 121, 83);
}

.crafting-title {
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 6px;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
    font-size: 8px;
}

.crafting-time-container {
    flex-grow: 1;
    position: absolute;
    top: 2px;
    right: 2px;
    bottom: 2px;
    flex-direction: row;
    background-color: rgba(255, 255, 255, 0.5);
    align-items: center;
    justify-content: center;
    align-self: center;
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
    border-bottom-right-radius: 5px;
    border-bottom-left-radius: 5px;
    font-size: 8px;
    width: 50px;
}

.crafting-time-icon {
    flex-grow: 0;
    width: 16px;
    height: 16px;
    background-image: url("project://database/Assets/BackwoodsLife/Res/Images/Icons/time-icon.png?fileID=21300000&guid=cd9ad196fb177a048973f8346ffd15b8&type=3#time-icon");
    left: 0;
    position: absolute;
}

.crafting-duration {
    margin-top: 0;
    margin-right: 4px;
    margin-bottom: 0;
    margin-left: 0;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
    position: absolute;
    right: 0;
}
